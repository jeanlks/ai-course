   "id": "6d1eba7c-b4ab-4eaa-b318-647370e67de9",
   "metadata": {},
   "outputs": [],
   "source": [
    "words = ['eating', 'playing', 'writing', 'writes', 'plays', 'eats']"
   ]
  },
  {
   "cell_type": "code",
